{
    "name": "dansup/php-activitystreams",
    "type": "library",
    "description": "Activity Streams is a simple specification used to describe social actions around the web. http://activitystrea.ms",
    "keywords": [
        "dansup",
        "activitystreams",
        "json"
    ],
    "homepage": "https://github.com/dansup/php-activitystreams",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Supernault",
            "email": "danielsupernault@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Dansup\\ActivityStreams\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dansup\\ActivityStreams\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
